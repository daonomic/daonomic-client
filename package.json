{
  "name": "daonomic-client",
  "version": "1.0.0",
  "description": "",
  "main": "source/index.js",
  "engines": {
    "node": "^8.9.0",
    "npm": "^5.5.1"
  },
  "scripts": {
    "start":
      "cross-env NODE_ENV=development API=development DEBUG_ENABLED=true webpack-dev-server",
    "storybook": "NODE_ENV=development start-storybook -p 9001",
    "prebuild": "rm -rf ./build && mkdir ./build && npm run build:copy-assets",
    "build": "cross-env NODE_ENV=production webpack",
    "build:copy-assets": "cp -R ./assets/* ./build",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true npm run build",
    "test": "jest --verbose",
    "lint": "eslint source webpack",
    "flow": "flow",
    "qa": "npm run lint && npm run test && npm run flow",
    "ensure-git-credentials":
      "git config --local user.name \"Dev\" && git config --local user.email \"dev@0v1se.com\"",
    "postinstall": "npm run ensure-git-credentials",
    "precommit": "npm run ensure-git-credentials && lint-staged && npm run qa",
    "format": "prettier --write \"./source/**/*.{js,css,json,md}\""
  },
  "lint-staged": {
    "*.{js,json,md,css}": ["prettier --write", "git add"]
  },
  "author": "Dev dev@0v1se.com",
  "license": "GPL-3.0",
