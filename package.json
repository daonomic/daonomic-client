  "scripts": {
    "start": "cross-env NODE_ENV=development ENVIRONMENT=development ENABLE_SOURCEMAPS=true webpack-dev-server",
    "prebuild": "rm -rf ./build && mkdir ./build",
    "build": "cross-env NODE_ENV=production webpack",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true npm run build",
    "locales:add": "lingui add-locale",
    "locales:extract": "lingui extract",
    "test": "npm-run-all --parallel test:*",
    "test:lint": "eslint source webpack",
    "test:types": "flow",
    "test:unit": "jest --silent",
    "pretest:e2e": "cross-env ENVIRONMENT=development E2E_TEST=true npm run build",
    "test:e2e": "node ./cypress/index.js",
    "ensure-git-credentials": "git config --local user.name \"Dev\" && git config --local user.email \"dev@0v1se.com\"",
    "postinstall": "npm run ensure-git-credentials",
    "precommit": "npm run ensure-git-credentials && lint-staged && npm run test:lint && npm run test:unit && npm run test:types",
    "format": "prettier --write \"./source/**/*.{js,css,json,md}\"",
    "ci:test": "sh ./ci/test.sh",
    "ci:build": "sh ./ci/build.sh"
  },
  "lint-staged": {
    "*.{js,json,md,css}": [
      "prettier --write",
      "git add"
    ]
  },
