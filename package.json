    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "cross-env": "5.1.4",
    "css-loader": "0.28.10",
    "eslint": "4.18.2",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-flowtype": "2.46.1",
